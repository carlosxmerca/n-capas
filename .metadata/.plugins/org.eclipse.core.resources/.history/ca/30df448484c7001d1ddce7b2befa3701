package Main;

import java.util.ArrayList;
import java.util.List;

public class MainClass {
	
	public static void main(String[] args)   
	{   
		// Lists
		System.out.println("--------- Lists ---------");
		
		List<Integer> numbers = new ArrayList<>();
		
		numbers.add(1);
		numbers.add(2);
		
		System.out.print(numbers);
	}

}
